import UIKit

var numeros = 0...100

for i in numeros{
    
    if i % 5 == 0{
        print("\(i) el numero" + " BINGO" )
    }
    if i % 2 == 0{
        print("\(i) el número es par")
    }else {
        print("\(i) el número es impar")
    }
    
    switch i{
        
    case 30...40:
        print("\(i) VIVA SWIFT¡¡¡" )
        
    default:
        break
    }
}
